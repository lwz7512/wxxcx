/*
  1. http://fontello.com/
  2. select icon and download .zip
  3. $ base64 fontello.ttf > fontbase64.txt
  4. copy fontello.css .icon-xxx to this file
  5. copy fontbase64.txt content to @font-face url(data:font/truetype;charset=utf-8;base64,...)
 */

@font-face {
  font-family: 'FontAwesome';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.fa {
  display: inline-block;
  font: normal normal normal 14px/1 FontAwesome;
  font-size: inherit;
  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}


